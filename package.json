{
  "name": "sonar-viewer",
  "displayName": "sonar-viewer",
  "description": "Visualizador de problemas do SonarCloud no VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sonar-viewer.showSonarCloudViewer",
    "onCommand:sonar-viewer.addSonarCommentsToFile"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sonar-viewer.showSonarCloudViewer",
        "title": "SonarViewer: Show Viewer"
      }
      ,
      {
        "command": "sonar-viewer.addSonarCommentsToFile",
        "title": "SonarViewer: Add Comments"
      }
    ],
    "configuration": {
      "title": "SonarCloud Viewer",
      "properties": {
        "sonarCloudViewer.accessToken": {
          "type": "string",
          "default": null,
          "description": "Token de acesso para o SonarCloud"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}